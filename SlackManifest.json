{
    "display_information": {
        "name": "Symphony",
        "description": "An AI assistant, but in Slack",
        "background_color": "#1d7c00"
    },
    "features": {
        "bot_user": {
            "display_name": "Symphony",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/symphony-help",
                "description": "Show the ablities Symphony can do",
                "should_escape": false
            },
            {
                "command": "/model",
                "description": "Switch to a different model",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "user": [
                "identity.basic"
            ],
            "bot": [
                "app_mentions:read",
                "channels:history",
                "channels:read",
                "chat:write",
                "commands",
                "files:read",
                "files:write",
                "groups:history",
                "groups:read",
                "im:history",
                "im:read",
                "im:write",
                "mpim:history",
                "mpim:read",
                "reactions:write",
                "users:read",
                "groups:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_mention",
                "member_joined_channel",
                "message.channels",
                "message.groups",
                "message.im",
                "message.mpim"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}